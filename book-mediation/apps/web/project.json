{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/web/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next dev -p 4200"
      },
      "outputs": [],
      "dependsOn": [],
      "continuous": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next dev -p 4200"
      },
      "outputs": [],
      "dependsOn": [],
      "continuous": true
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next build"
      },
      "configurations": {
        "qa": {
          "command": "dotenv -e .env.qa -- next build"
        },
        "production": {
          "command": "dotenv -e .env.production -- next build"
        }
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next start -p 4200"
      },
      "configurations": {
        "qa": {
          "command": "dotenv -e .env.qa -- next start -p 4200"
        },
        "production": {
          "command": "dotenv -e .env.production -- next start -p 4200"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
